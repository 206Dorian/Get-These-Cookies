<!-- Loop over each object in the projects array -->
{{#each posts }}
<card = class "card">
  <br>
  <div class= "is primary">
    <h2>{{title}}</h2>
  </div>
<br>
  <div class="is link is-2">
    <p>{{postbody}}</p>
  </div>
<br>
  <div class= "is primary">
    <p>{{date_created}}</p>
  </div>

</card>
{{/each}}